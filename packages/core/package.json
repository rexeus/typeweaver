{
  "name": "@rexeus/typeweaver-core",
  "version": "0.0.4",
  "description": "Core TypeScript and Zod utilities for typeweaver API framework",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE",
    "NOTICE"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "build": "pkgroll --clean-dist && cp ../../LICENSE ../../NOTICE ./dist/",
    "prepack": "npm run build",
    "preversion": "npm run build"
  },
  "keywords": [
    "api",
    "spec",
    "definition",
    "typescript",
    "zod",
    "generator",
    "typeweaver"
  ],
  "author": "Dennis Wentzien <dw@rexeus.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rexeus/typeweaver.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/rexeus/typeweaver/issues"
  },
  "homepage": "https://github.com/rexeus/typeweaver#readme",
  "peerDependencies": {
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "zod": "^4.1.5"
  }
}
