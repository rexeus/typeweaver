{
  "name": "@rexeus/typeweaver-zod-to-ts",
  "version": "0.6.2",
  "description": "Transforms Zod v4 schemas into precise TypeScript shapes. Powered by Typeweaver ðŸ§µâœ¨",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE",
    "NOTICE"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "format": "oxfmt",
    "build": "tsdown && cp ../../LICENSE ../../NOTICE ./dist/",
    "prepack": "npm run build",
    "preversion": "npm run build"
  },
  "keywords": [
    "zod",
    "typescript",
    "type",
    "conversion",
    "generator",
    "typeweaver"
  ],
  "author": "Dennis Wentzien <dw@rexeus.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rexeus/typeweaver.git",
    "directory": "packages/zod-to-ts"
  },
  "bugs": {
    "url": "https://github.com/rexeus/typeweaver/issues"
  },
  "homepage": "https://github.com/rexeus/typeweaver#readme",
  "dependencies": {
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "zod": "^4.3.0"
  },
  "devDependencies": {
    "zod": "^4.3.6"
  }
}
