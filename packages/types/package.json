{
  "name": "@rexeus/typeweaver-types",
  "version": "0.5.1",
  "description": "Generates request and response types plus validators aligned with your API contract. Powered by Typeweaver ðŸ§µâœ¨",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE",
    "NOTICE"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "format": "oxfmt",
    "build": "tsdown && mkdir -p ./dist/templates ./dist/lib && cp -r ./src/templates/* ./dist/templates/ && node scripts/compile-lib.mjs && cp src/lib-declarations/*.d.ts dist/lib/ && cp src/lib/index.ts dist/lib/ && cp ../../LICENSE ../../NOTICE ./dist/",
    "test": "vitest --run",
    "prepack": "npm run build",
    "preversion": "npm run build"
  },
  "keywords": [
    "api",
    "spec",
    "definition",
    "typescript",
    "zod",
    "generator",
    "typeweaver"
  ],
  "author": "Dennis Wentzien <dw@rexeus.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rexeus/typeweaver.git",
    "directory": "packages/types"
  },
  "bugs": {
    "url": "https://github.com/rexeus/typeweaver/issues"
  },
  "homepage": "https://github.com/rexeus/typeweaver#readme",
  "peerDependencies": {
    "@rexeus/typeweaver-core": "workspace:^",
    "@rexeus/typeweaver-gen": "workspace:^"
  },
  "devDependencies": {
    "@rexeus/typeweaver-core": "workspace:^",
    "@rexeus/typeweaver-gen": "workspace:^",
    "oxc-transform": "^0.115.0",
    "test-utils": "file:../test-utils"
  },
  "dependencies": {
    "@rexeus/typeweaver-zod-to-ts": "workspace:^",
    "case": "^1.6.3"
  }
}
