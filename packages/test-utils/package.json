{
  "name": "test-utils",
  "description": "Test utils for typeweaver packages",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "test-project:gen": "pnpm exec typeweaver generate --output src/test-project/output --input src/test-project/definition --plugins clients,aws-cdk,hono,express --shared src/test-project/definition/shared",
    "test:gen": "pnpm run test-project:gen"
  },
  "keywords": [
    "typeweaver",
    "test",
    "utils"
  ],
  "devDependencies": {
    "@hono/node-server": "^1.19.7",
    "@rexeus/typeweaver": "workspace:^",
    "@rexeus/typeweaver-aws-cdk": "workspace:^",
    "@rexeus/typeweaver-clients": "workspace:^",
    "@rexeus/typeweaver-core": "workspace:^",
    "@rexeus/typeweaver-express": "workspace:^",
    "@rexeus/typeweaver-gen": "workspace:^",
    "@rexeus/typeweaver-hono": "workspace:^",
    "@rexeus/typeweaver-types": "workspace:^",
    "@types/express": "5.0.6",
    "@types/supertest": "^6.0.3",
    "axios": "^1.13.2",
    "case": "^1.6.3",
    "deepmerge": "^4.3.1",
    "express": "^5.2.1",
    "get-port": "^7.1.0",
    "hono": "^4.11.3",
    "supertest": "^7.1.4",
    "zod": "^4.3.5"
  }
}
