{
  "name": "@rexeus/typeweaver-gen",
  "version": "0.0.0",
  "description": "Code generation engine and utilities for TypeWeaver plugins",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "build": "pkgroll --clean-dist",
    "prepack": "npm run build",
    "preversion": "npm run build"
  },
  "keywords": [
    "api",
    "spec",
    "definition",
    "typescript",
    "zod",
    "generator",
    "typeweaver"
  ],
  "author": "Dennis Wentzien <dw@rexeus.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rexeus/typeweaver.git",
    "directory": "packages/gen"
  },
  "bugs": {
    "url": "https://github.com/rexeus/typeweaver/issues"
  },
  "homepage": "https://github.com/rexeus/typeweaver#readme",
  "peerDependencies": {
    "@rexeus/typeweaver-core": "*"
  },
  "devDependencies": {
    "@rexeus/typeweaver-core": "workspace:*"
  }
}
